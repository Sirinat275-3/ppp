# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index()
GET     /pormotions                 controllers.Application.pormotions()
GET     /oder                       controllers.Application.oder()
GET     /product                    controllers.Application.product()
GET     /pt                         controllers.Application.pt()
GET     /inputIce                   controllers.Application.inputIce()
GET     /sirinat                    controllers.Application.sirinat()
GET     /water
POST    /postIce                    controllers.Application.PostIce()



# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
